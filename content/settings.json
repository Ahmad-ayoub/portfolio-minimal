    "social": {
      "medium": "",
      "mail": "",
      "github": "https://github.com/Ahmad-ayoub",
      "linkedin": "https://www.linkedin.com/in/ahmad-ayoub/"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "My Portfolio" },
